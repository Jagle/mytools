<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_window_focused="false">
        <shape>
            <gradient android:startColor="#ffe0e0e0" android:endColor="#ffe0e0e0" />
            <corners android:topLeftRadius="10.0px" android:topRightRadius="10.0px" android:bottomLeftRadius="10.0px" android:bottomRightRadius="10.0px" />
            <stroke android:width="2.0dip" android:color="#ff418acc" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ff418acc" android:endColor="#ff418acc" />
            <corners android:topLeftRadius="10.0px" android:topRightRadius="10.0px" android:bottomLeftRadius="10.0px" android:bottomRightRadius="10.0px" />
            <stroke android:width="1.0dip" android:color="#ff418acc" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <gradient android:startColor="#ff418acc" android:endColor="#ff418acc" />
            <corners android:topLeftRadius="10.0px" android:topRightRadius="10.0px" android:bottomLeftRadius="10.0px" android:bottomRightRadius="10.0px" />
            <stroke android:width="1.0dip" android:color="#ff418acc" />
        </shape>
    </item>
</selector>